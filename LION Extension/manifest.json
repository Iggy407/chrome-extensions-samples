{
  "manifest_version": 3,
  "name": "Excel File Detector",
  "version": "1.1",
  "description": "Detects Excel files on a webpage and alerts their presence.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Detect Excel Files"
  }
}
